<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="woo.sopt22.meowbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.Home.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.MeowBoxStory.MeowBoxStoryActivity"
            android:label="@string/title_activity_meow_box_story"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".View.MeowBoxReview.MeowBoxReviewActivity"
            android:label="@string/title_activity_meow_box_review"
            android:theme="@style/AppTheme" />
        <activity android:name=".View.Order.OrderActivity" />
        <activity
            android:name=".View.MeowBoxDetail.MeowBoxDetailActivity"
            android:label="@string/title_activity_meow_box_detail"
            android:theme="@style/AppTheme" />
        <activity android:name=".View.MyPage.MyPageActivity" />
        <activity
            android:name=".View.Order.OrderFirstActivity"
            android:label="@string/title_activity_order_first"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>